<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_margin="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_logs_category"
        android:textStyle="bold"
        android:textColor="?titleStaticColor"
        tools:text="Meal"/>

    <TextView
        android:layout_margin="16dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/tv_logs_total"
        android:textAlignment="textEnd"
        tools:text="Rp.12391239"
        app:layout_constraintTop_toTopOf="@id/tv_logs_category"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_logs_category"
        app:layout_constraintBottom_toBottomOf="@id/tv_logs_category"
        />


</androidx.constraintlayout.widget.ConstraintLayout>